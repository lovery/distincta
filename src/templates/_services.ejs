<div class="row services_content scrollpoint sp-effect5">
  <% for (service of services) { %>
    <div class="media col col-md-4 col-xs-12 ">
    <div class="service-container">
      <div class="icon-container"><div class="icon"><i class="fa <%- service.icon %> fa-2x"></i></div></div>
      <div class="heading-container">
        <h3 class="title">
          <span><%- service.name %></span>
        </h3>
      </div>
      <div class="content">
        <div class="body"><p><%- service.content %></p></div>
      </div>
    </div>
    </div>
  <% } %>
</div>
